<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jk.mapper.PositionMapper">

    <delete id="deletes">
        delete  from t_position_zw where positionid = #{ids}
    </delete>

    <select id="selectPositId" resultType="com.jk.model.PositionModel">
         select * from t_position_zw where positionid = #{id}
    </select>

    <select id="queryPosition" resultType="com.jk.model.PositionModel">
         select * from t_position_zw where positionid = #{id}
    </select>
    <select id="querlposition" resultType="com.jk.model.PositionModel">
         select * from t_position_zw where positionid = #{id}
    </select>

    <select id="querlpositionn" resultType="com.jk.model.PositionModel">
          select * from t_position_zw
    </select>
</mapper>